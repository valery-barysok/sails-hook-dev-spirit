    "fs-extra": "0.24.0",
    "lodash": "3.10.1",
    "pretty-bytes": "2.0.1"
  },
  "devDependencies": {
    "coffee-script": "1.10.0",
    "coveralls": "2.11.4",
    "istanbul": "^0.3.19",
    "mocha": "2.3.2",
    "sails": "0.11.0",
    "should": "7.1.0"
  }
